65. Write a C program to store hospital patient details.
#include <stdio.h>

struct patient {
    int patientId;
    char name[20];
    int age;
};

int main() {
    struct patient p;

    printf("Enter Patient ID: ");
    scanf("%d", &p.patientId);
    printf("Enter Name: ");
    scanf("%s", p.name);
    printf("Enter Age: ");
    scanf("%d", &p.age);

    printf("\nPatient Details:\n");
    printf("ID: %d\nName: %s\nAge: %d", p.patientId, p.name, p.age);

    return 0;
}
